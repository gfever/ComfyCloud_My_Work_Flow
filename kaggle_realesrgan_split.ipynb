    "    \"            lf.write('\\n=== STDERR ===\\n')\\n\",\n",
    "    \"            lf.write(res.stderr or '')\\n\",\n",
    "    \"        print('Return code:', getattr(res, 'returncode', None))\\n\",\n",
    "    \"        if res.returncode == 0:\\n\",\n",
