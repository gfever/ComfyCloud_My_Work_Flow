    "    \"        with open(path, 'r', encoding='utf-8', errors='ignore') as f:\\n\",\n",
    "    \"            data = f.read().splitlines()\\n\",\n",
    "    \"        return '\\n'.join(data[-n:])\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        return f'Could not read {path}: {e}'\\n\",\n",
    "    \"print('Helpers ready')\"\n",
    "   ]\n",
    "  },\n",
