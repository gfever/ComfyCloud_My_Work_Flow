    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# Minimal helpers used by later cells\\n\",\n",
    "    \"import os, sys, shutil, subprocess, textwrap\\n\",\n",
    "    \"def print_separator(nl_before=True):\\n\",\n",
    "    \"    SEP = '=' * 60\\n\",\n",
    "    \"    if nl_before:\\n\",\n",
    "    \"        print('\\n' + SEP)\\n\",\n",
    "    \"    else:\\n\",\n",
    "    \"        print(SEP + '\\n')\\n\",\n",
