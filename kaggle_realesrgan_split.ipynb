    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# WARNING: this cell runs the Real-ESRGAN inference subprocess. Run only after verifying run_cmd above.\\n\",\n",
    "    \"if run_cmd is None:\\n\",\n",
    "    \"    print('run_cmd is not defined. Build it in the previous cell first.')\\n\",\n",
    "    \"else:\\n\",\n",
    "    \"    print('Running:', run_cmd)\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        res = subprocess.run(run_cmd, capture_output=True, text=True, timeout=3600)\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        print('Subprocess failed to start or timed out:', e)\\n\",\n",
    "    \"    else:\\n\",\n",
    "    \"        with open(INFERENCE_LOG, 'a', encoding='utf-8') as lf:\\n\",\n",
