    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# This cell attempts to create a minimal torchvision stub and re-run run_cmd with PYTHONPATH adjusted.\\n\",\n",
    "    \"# Run only if logs indicate missing 'functional_tensor' or similar import errors.\\n\",\n",
    "    \"try:\\n\",\n",
    "    \"    if 'functional_tensor' not in ''.join([tail(INFERENCE_LOG, 20)]).lower() and 'functional_tensor' not in (globals().get('last_inference_stderr','') or '').lower():\\n\",\n",
    "    \"        print('Log does not show functional_tensor error — inspect', INFERENCE_LOG)\\n\",\n",
    "    \"    else:\\n\",\n",
    "    \"        print('Preparing local torchvision stub...')\\n\",\n",
    "    \"        stub_root = os.path.join(WORKSPACE, 'torchvision_stub')\\n\",\n",
    "    \"        torchvision_pkg = os.path.join(stub_root, 'torchvision')\\n\",\n",
    "    \"        transforms_pkg = os.path.join(torchvision_pkg, 'transforms')\\n\",\n",
    "    \"        shutil.rmtree(stub_root, ignore_errors=True)\\n\",\n",
    "    \"        os.makedirs(transforms_pkg, exist_ok=True)\\n\",\n",
    "    \"        # minimal files\\n\",\n",
    "    \"        with open(os.path.join(torchvision_pkg, '__init__.py'), 'w', encoding='utf-8') as f:\\n\",\n",
