    "    \"            for src in src_candidates:\\n\",\n",
    "    \"                try:\\n\",\n",
    "    \"                    if src and os.path.exists(src):\\n\",\n",
    "    \"                        shutil.copy(src, weights_dir)\\n\",\n",
    "    \"                        lf.write(f'Copied upscale model from {src} to {weights_dir}\\n')\\n\",\n",
    "    \"                        print('Copied model from', src)\\n\",\n",
    "    \"                        copied = True\\n\",\n",
    "    \"                        break\\n\",\n",
    "    \"                except Exception as _c_e:\\n\",\n",
    "    \"                    lf.write(f'WARNING: failed to copy model from {src}: {_c_e}\\n')\\n\",\n",
    "    \"            if not copied:\\n\",\n",
    "    \"                lf.write('NOTE: no upscale model found in candidate paths; weights/ may be missing the model\\n')\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        print('⚠️ Error preparing weights:', e)\\n\",\n",
