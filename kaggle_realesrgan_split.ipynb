    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# If no shim found above, generate a conservative shim that injects netscale/args and execs inference_realesrgan.py\\n\",\n",
    "    \"if os.path.exists(os.path.join(WORKSPACE, 'Real-ESRGAN')):\\n\",\n",
    "    \"    if not (globals().get('wrote') and os.path.exists(os.path.join(os.getcwd(), 'inference_with_shim_full.py'))):\\n\",\n",
    "    \"        shim_path = os.path.join(os.getcwd(), 'inference_with_shim_full.py')\\n\",\n",
    "    \"        shim_lines = [\\n\",\n",
    "    \"            \\\"import sys, os, runpy, types, re\\\",\\n\",\n",
    "    \"            \\\"# Minimal compatibility shim: define netscale/model/outscale and exec the original script\\\",\\n\",\n",
    "    \"            \\\"def _infer_netscale(argv):\\\",\\n\",\n",
    "    \"            \\\"    for i,a in enumerate(argv):\\\",\\n\",\n",
    "    \"            \\\"        if a in ('-n','--name','--model') and i+1<len(argv):\\\",\\n\",\n",
    "    \"            \\\"            m = re.match(r'(\\\\\\\\d+)x', argv[i+1])\\\",\\n\",\n",
    "    \"            \\\"            if m:\\\",\\n\",\n",
    "    \"            \\\"                try: return int(m.group(1))\\\\n                except: pass\\\",\\n\",\n",
    "    \"            \\\"    for i,a in enumerate(argv):\\\",\\n\",\n",
    "    \"            \\\"        if a in ('-s','--scale') and i+1<len(argv):\\\",\\n\",\n",
