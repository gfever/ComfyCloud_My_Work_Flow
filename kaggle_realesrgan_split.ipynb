    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# Copy model into weights/ if possible (writes into INFERENCE_LOG)\\n\",\n",
    "    \"if os.path.exists(upscale_model) and os.path.exists(os.path.join(WORKSPACE, 'Real-ESRGAN')):\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        with open(INFERENCE_LOG, 'a', encoding='utf-8') as lf:\\n\",\n",
    "    \"            lf.write('Preparing weights directory\\n')\\n\",\n",
